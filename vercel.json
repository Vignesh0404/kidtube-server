{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users"
    },
    {
      "src": "/api/videos/(.*)",
      "dest": "/api/videos"
    },
    {
      "src": "/api/comments/(.*)",
      "dest": "/api/comments"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
